{
	"ifdrop": {
		"prefix": "ifdrop",
		"body": ["IF OBJECT_ID('$1', 'U') IS NOT NULL",
			"DROP TABLE $1;"],
		"description": "drop table if already exists"
	}, 
	"ifdroptmp": {
		"prefix": "ifdroptmp",
		"body": ["IF OBJECT_ID('tempdb..#$1', 'U') IS NOT NULL",
			"DROP TABLE #$1;"],
		"description": "drop table if already exists"
	}
}
